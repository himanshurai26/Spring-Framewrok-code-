<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
" xmlns:context="http://www.springframework.org/schema/context">
    
    
 <bean id="b" class="com.spring.autowiring.B"> </bean>
    
    <bean id="a" class="com.spring.autowiring.A" autowire="byName"></bean> 
    
    <bean name="address" class="com.spring.autowiring.Address">
        <property name="name" value="Himanshu Kumar"/>
        <property name="address" value="Kalkaji new Delhi"/>
    </bean>
    
    <bean name="emp1" class="com.spring.autowiring.Emp" autowire="byName"/>
   


</beans>


