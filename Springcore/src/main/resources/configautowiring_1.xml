<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
" xmlns:context="http://www.springframework.org/schema/context">
    
    
 <context:annotation-config/>
    
    <bean name="address" class="com.spring.autowiring.annotation.Address">
        <property name="name" value="Himanshu Kumar"/>
        <property name="address" value="Kalkaji new Delhi"/>
    </bean>
    
    <bean name="emp1" class="com.spring.autowiring.annotation.Emp"/>
   


</beans>


