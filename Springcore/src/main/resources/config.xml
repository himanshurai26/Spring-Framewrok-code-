<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
">
    
    <bean id="student1" class="com.spring.springcore.Student">
        <property name="StudentId" value="23"/>
        <property name="StudentName" value="Himanshu Kumar"/>
        <property name="StudentAddress" value="New Delhi"/>
        
        
    </bean>
    
    <bean id="student2" class="com.spring.springcore.Student">
        <property name="StudentId" value="24"/>
        <property name="StudentName" value="Priyanshu Kumar"/>
        <property name="StudentAddress" value="Bihar"/>
           
        
    </bean>

</beans>
